        "keyColor": "33",
      },
      {
        "type": "cpu",
        "format": "{1} ({3}) @ {7} GHz",
        "key": " ├  ",
        "keyColor": "33",
      },
      {
        "type": "gpu",
        "format": "{1} {2}",            
        "key": " ├ 󰢮 ",
        "keyColor": "33",
      },
      {
        "type": "memory",
        "key": " ├  ",
        "keyColor": "33",
      },
      {
        "type": "disk",
        "key": " └ 󰋊 ",
        "keyColor": "33",
      },
      "break",
      "break",
